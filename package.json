{
  "name": "steam-market",
  "version": "1.0.0",
  "description": "Steam community market API client",
  "keywords": [
    "steam",
    "community",
    "market",
    "api",
    "client"
  ],
  "license": "MIT",
  "author": "Vladislav Puzyrev",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladislav-puzyrev/steam-market.git"
  },
  "scripts": {
    "build": "yarn run clean:build && tsc",
    "docs": "yarn run clean:docs && yarn run docs:typedoc && yarn run docs:concat",
    "docs:typedoc": "typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs --name API src/index.ts",
    "docs:concat": "concat-md --hide-anchor-links docs > README.md",
    "clean": "yarn run clean:build && yarn run clean:docs",
    "clean:build": "rimraf build",
    "clean:docs": "rimraf docs",
    "test": "yarn run test:typescript && yarn run test:eslint && yarn run test:ncu",
    "test:typescript": "tsc --noEmit",
    "test:eslint": "eslint .",
    "test:ncu": "ncu --errorLevel 2"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "currency-codes": "^2.1.0",
    "iso-639-1": "^2.1.15",
    "proxy-string-parser": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "concat-md": "^0.5.0",
    "eslint": "^8.29.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-check-updates": "^16.4.3",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.3"
  }
}
